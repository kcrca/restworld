<%namespace name="base"  file="base.mcftmpl"/>\
<%
    rabbits = ( "Brown",
	"White",
	"Black",
	"Black & White",
	"Gold",
	"Salt & Pepper",
	"Killer Rabbit (unused)",
    )
%>

<%base:loop collection="${rabbits}" args="when,i,thing">\
<% type = 99 if thing.startswith("Killer") else i %>
${when(i)} execute as @e[tag=rabbits] run data merge entity @s {RabbitType:${type},CustomName:${text(thing)}}\
</%base:loop>\
