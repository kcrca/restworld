<%namespace name="base"  file="base.mcftmpl"/>
<%
    types = range(0, 3)
    threes = [f for f in fishes if len(f[1]) == 3]
%>

<%base:loop collection="${types}" args="when,i,thing">
% for f in threes:
<%
    tag, variants = f
    v = variants[thing]
%>\
${when(i)} data merge entity @e[tag=${tag},limit=1] {Variant:${v[0]},CustomName:${text(v[1])}}
% endfor
</%base:loop>
