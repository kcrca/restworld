<%namespace name="base"  file="base.mcftmpl"/>
<%
    blocks = to_nicknamed("Fence", woods + ("Nether Brick",)) + to_nicknamed("Cobblestone Wall", ("", "Mossy"))
%>

<%base:loop collection="${blocks}" args="when,i,item">
${when(i)} fill ~ ~2 ~ ~5 ~3 ~7 ${item.full_id()} replace #allstuff:fences
% try:
${when(i)} data merge block ~5 ~2 ~5 {Text2:${text(item.nickname)},Text3:${text(item.kind)}}
% except:
${when(i)} data merge block ~5 ~2 ~5 {Text2:${text(item.name)},Text3:${text("")}}
% endtry
</%base:loop>
