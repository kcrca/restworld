<%namespace file="effect_base.mcftmpl" import="*"/>
<%namespace name="base"  file="../base.mcftmpl"/>
<% animals = (Thing("ocelot"), Thing("Horse"), Thing("Llama")) %>

<%base:loop collection="${animals}" args="when,i,thing">
