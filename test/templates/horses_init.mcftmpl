<%namespace name="base"  file="base.mcftmpl"/>
<%
    horses = (
	Thing("White"),
	Thing("Creamy"),
	Thing("Chestnut"),
	Thing("Brown"),
	Thing("Black"),
	Thing("Gray"),
	Thing("Dark Brown"),
    )
    others = (
	Thing("Mule"),
	Thing("Donkey"),
	Thing("Skeleton Horse"),
	Thing("Zombie Horse"),
    )
%>

${base.pair_init(z_delta_init=-2, adult_x_init=-2.5, kid_x_init=0.5, rotation_init=270)}\
% for horse in horses:
${base.pair(horse.name, id="horse", nbt="Variant:%d" % loop.index)}\
% endfor
% for horse in others:
${base.pair(horse.name)}\
% endfor
