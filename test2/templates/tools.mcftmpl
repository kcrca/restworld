<%namespace name="base"  file="base.mcftmpl"/>
<%
    things = (
	(Thing("Shears"), Thing("Fishing Rod")),
	(Thing("Trident"), Thing("Iron Sword")),
	(Thing("Iron Pickaxe"), Thing("Iron Axe")),
	(Thing("Iron Shovel"), Thing("Iron Hoe")),
    )
%>

<%base:loop collection="${things}" args="when,i,thing">
<% right, left = thing %>\
${when(i)} data merge entity @e[tag=armor_stand,limit=1] {HandItems:[{id:${right.id},Count:1},{id:${left.id},Count:1}]}
</%base:loop>
