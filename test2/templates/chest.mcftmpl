<%namespace name="base"  file="base.mcftmpl"/>

<%base:loop collection="${(0, 1)}" args="when,i,thing">
% if thing == 0:
${when(i)} setblock ~1 ~2 ~ minecraft:chest[facing=north,type=left]
${when(i)} setblock ~0 ~2 ~ air
% else:
${when(i)} setblock ~1 ~2 ~ air
${when(i)} setblock ~0 ~2 ~ minecraft:trapped_chest[facing=north,type=right] 
% endif
</%base:loop>
