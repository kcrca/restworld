<%namespace name="base" file="../base.mcftmpl"/>
<% blocks = colors + (Thing("Glass Pane"), ) %>

<%base:loop collection="${blocks}" args="when,i,thing">
% if thing in colors:
${base.fencelike(when, i, "minecraft:%s_stained_glass_pane" % thing.id, thing.name, "Stained Glass")}\
% else:
${base.fencelike(when, i, thing.full_id(), thing.name, "")}\
% endif
</%base:loop>
