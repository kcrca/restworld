<%namespace name="base" file="../base.mcftmpl"/>
<%
    blocks = [
	Thing("Grass Block"),
	Thing("Dirt"),
	Thing("Coarse Dirt"),
	Thing("Podzol"),
	Thing("Sand"),
	Thing("Red Sand"),
    ]
%>

<%base:bounce collection="${blocks}" args="when,i,thing">
${when(i)} setblock ~ ~2 ~-1 ${thing.full_id()}
</%base:bounce>
