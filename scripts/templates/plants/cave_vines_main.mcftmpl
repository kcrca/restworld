<%namespace name="base" file="../base.mcftmpl"/>

<%base:loop collection="${('true', 'false')}" args="when,i,thing">\
${when(i)} fill ~0 ~2 ~0 ~0 ~3 ~0 cave_vines[berries=${thing}]
${when(i)} data merge block ~1 ~4 ~0 {Text3:${text("With%s Berries" % ('' if thing == 'true' else 'out'))}}
</%base:loop>
