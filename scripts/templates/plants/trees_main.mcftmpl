<%namespace name="base" file="../base.mcftmpl"/>

<%base:loop collection="${woods}" args="when,i,thing">\
${when(i)} data merge block ~-1 ~0 ~-1 {mode:"LOAD",name:"restworld:${thing.lower().replace(' ', '_')}_trees"}
${when(i)} setblock ~-1 ~-1 ~-1 redstone_block
${when(i)} setblock ~-1 ~-1 ~-1 air
${when(i)} setblock ~1 ~2 ~7 minecraft:oak_wall_sign[facing=west]{Text2:"\"${thing} Trees\""} replace
</%base:loop>
