<%namespace name="base" file="../base.mcftmpl"/>

<%base:loop collection="${('red', 'brown')}" args="when,i,thing">\
${when(i)} data merge block ~-1 ~0 ~-1 {mode:"LOAD",name:"restworld:${thing}_mushroom"}
${when(i)} setblock ~-1 ~-1 ~-1 redstone_torch
${when(i)} setblock ~-1 ~-1 ~-1 air
</%base:loop>
