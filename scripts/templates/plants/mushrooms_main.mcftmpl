<%namespace name="base" file="../base.mcftmpl"/>
<% mushrooms = (Thing("Red Mushroom"), Thing("Brown Mushroom")) %>

<%base:loop collection="${mushrooms}" args="when,i,thing">\
${when(i)} setblock ~ ~3 ~ air
${when(i)} setblock ~ ~3 ~ ${thing.full_id()}
${when(i)} data merge block ~1 ~2 ~ {Text2:${text(thing.name)}}
</%base:loop>
