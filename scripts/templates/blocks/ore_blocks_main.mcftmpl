<%namespace import="*" file="blocks_base.mcftmpl"/>
<%
    types = ("Coal", "Iron", "Gold", "Diamond", "Lapis", "Emerald", "Redstone", "Nether Quartz", "Nether Gold")
    ore_blocks = ["%s Block" % x for x in filter(lambda x: not 'Nether' in x, types)] + ["Quartz Block", "Gold Block"]
%>
${blocks(tuple("%s Ore" % t for t in types), ore_blocks, dz=3)}
