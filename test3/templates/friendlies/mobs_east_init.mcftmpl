<%namespace name="base" file="../base.mcftmpl"/>\
<% llamas = ( Thing("Creamy"), Thing("White"), Thing("Brown"), Thing("Gray")) %>

${base.mobs_init(z_delta_init=2, kid_x_init=-2.2, adult_x_init=0.20, rotation_init=90, tags_init=("friendlies",))}\
% for llama in llamas:
${base.mobs(llama.name, id="llama", nbt="Variant:%d" % loop.index)}\
% endfor
summon minecraft:llama_spit ~3 ~3 ~0 {CustomName:"\"Llama Spit\"",TXD:0,TYD:0,TZD:0,Steps:0,NoAI:True,Motion:[0,0,0],NoGravity:True}
${base.mobs("Rabbit", tags=("rabbits",))}

${base.mobs("Bat", y_add=0.5, do_kid=True)}
${base.adult(Thing("Bat"), 0, 1.5, 10, 270, nbt="BatFlags:1", tags=("bat", "sleeping_bat", "friendlies"))}
